<select name="variable_{{big_index}}_{{small_index}}">
        {% for key, act in list %}
        {% if act is iterable %}
            <optgroup> {{key}}  </optgroup>
            {% for k, a in act %}
            <option value="{{ k }}"> {{a}}  </option>
            {% endfor %}
        {% else%}
        <option value="{{ key }}"> {{act}}  </option>
        {% endif %}
        {% endfor %}
</select>