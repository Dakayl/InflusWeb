{% for key,p in parameters %}
<label for="variable_{{index}}_{{loop.index}}">{{key}} :</label>
{% if p == "text" %}
    <input type="text" value="" id="variable_{{index}}_{{loop.index}}" name="variable_{{index}}_{{loop.index}}" />
{% elseif p == "string" %}
    <input type="text" value="" id="variable_{{index}}_{{loop.index}}" name="variable_{{index}}_{{loop.index}}" />
{% elseif p == "level" %}
    <input type="number" min="1" max="5" step="1" value="" id="variable_{{index}}_{{loop.index}}" name="variable_{{index}}_{{loop.index}}" />
{% elseif p == "list_learn_mentor" %}
    {{ include('CamaInflusBundle:Fiche:select.html.twig', { "list":list_learn_mentor, 'big_index': index, 'small_index':loop.index}) }}
{% elseif p == "list_learn_nomentor" %}
    {{ include('CamaInflusBundle:Fiche:select.html.twig', { "list":list_learn_nomentor, 'big_index': index, 'small_index':loop.index}) }}
{% elseif p == "list_ville" %}
    {{ include('CamaInflusBundle:Fiche:select.html.twig', { "list":list_ville, 'big_index': index, 'small_index':loop.index}) }}
{% elseif p == "list_refuge" %}
    {{ include('CamaInflusBundle:Fiche:select.html.twig', { "list":list_refuge, 'big_index': index, 'small_index':loop.index}) }}
{% elseif p == "list_historique" %}
    {{ include('CamaInflusBundle:Fiche:select.html.twig', { "list":list_historique, 'big_index': index, 'small_index':loop.index}) }}
{% elseif p == "list_serv" %}
    {{ include('CamaInflusBundle:Fiche:select.html.twig', { "list":list_serv, 'big_index': index, 'small_index':loop.index}) }}
{% endif %}
    &nbsp;<br />
{% endfor %}
<label for="justif_action_s_{{index}}">Informations / Justification :</label>
<textarea id="justif_action_s_{{index}}" name="justif_action_s_{{index}}" cols="60" rows="1"></textarea>
